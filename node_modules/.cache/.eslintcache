tle.toLowerCase()] = items;\n      return acc;\n    }, {});\n  \n    return categoryMap;\n  };\n  \n  export const createAuthUserWithEmailAndPassword = async (email, password) => {\n    if (!email || !password) return;\n  \n    return await createUserWithEmailAndPassword(auth, email, password);\n  };","/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/cart-dropdown/cart-dropdown.jsx",[],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/cart-icon/cart-icon.jsx",[],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/product-cart/product-card.jsx",[],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/categories/categories.jsx",[],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/checkout-item/checkout-item.jsx",[],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/sign-in-form/sign-in.jsx",["151","152","153"],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/sign-up-form/sign-up.jsx",["154"],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/button/button.jsx",[],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/cart-item/cart-item.jsx",[],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/form-input/form-input.jsx",[],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/context/categoriesContext.jsx",["155","156"],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/shop-data.js",[],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/routes/categories-preview/categories-preview.jsx",[],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/routes/category/category.jsx",[],[],"/Users/luwam/Desktop/Tutorial/React/ecommerce-page-with-react/src/components/directory-item/directory-item.jsx",